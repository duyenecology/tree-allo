	keywords = {allometry, finite-size scaling, old-growth forests, self-thinning},
	pages = {20122375},
	annote = {Publisher: Royal Society},
	annote = {Publisher: Royal Society},
	annote = {Publisher: Royal Society},
	file = {Full Text PDF:/Users/tpc/Zotero/storage/3SJNDGG6/Anfodillo et al. - 2013 - An allometry-based approach for understanding fore.pdf:application/pdf},
}

@article{Zhang2023,
