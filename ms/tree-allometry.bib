	keywords = {community level modeling, cross validation, generalized linear models, mixture models, species archetypes, species distribution models},
	pages = {1913--1919},
	file = {Full Text PDF:/Users/tpc/Zotero/storage/UA464JHJ/Hui et al. - 2013 - To mix or not to mix comparing the predictive per.pdf:application/pdf;Snapshot:/Users/tpc/Zotero/storage/MSSSD6I7/12-1322.html:text/html},
}

