	note = {Publisher: International Society for Bayesian Analysis},
	pages = {667--718},
	file = {Full Text PDF:/Users/tpc/Zotero/storage/XFLBV4CV/Vehtari et al. - 2021 - Rank-Normalization, Folding, and Localization An .pdf:application/pdf},
}

