	urldate = {2024-01-13},
	journal = {Remote Sensing},
	author = {Wang, Pei and Gan, Xiaozheng and Zhang, Qing and Bu, Guochao and Li, Li and Xu, Xiuxian and Li, Yaxin and Liu, Zichu and Xiao, Xiangming},
