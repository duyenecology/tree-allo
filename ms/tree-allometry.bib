@article{Mäkelä2006,
	title = {Crown {Ratio} {Influences} {Allometric} {Scaling} in {Trees}},
	volume = {87},
	copyright = {© 2006 by the Ecological Society of America},
	issn = {1939-9170},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1890/0012-9658%282006%2987%5B2967%3ACRIASI%5D2.0.CO%3B2},
	doi = {10.1890/0012-9658(2006)87[2967:CRIASI]2.0.CO;2},
	abstract = {Allometric theories suggest that the size and shape of organisms follow universal rules, with a tendency toward quarter-power scaling. In woody plants, however, structure is influenced by branch death and shedding, which leads to decreasing crown ratios, accumulation of heartwood, and stem and branch tapering. This paper examines the impacts on allometric scaling of these aspects, which so far have been largely ignored in the scaling theory. Tree structure is described in terms of active and disused pipes arranged as an infinite branching network in the crown, and as a tapering bundle of pipes below the crown. Importantly, crown ratio is allowed to vary independently of crown size, the size of the trunk relative to the crown deriving from empirical results that relate crown base diameter to breast height diameter through crown ratio. The model implies a scaling relationship in the crown which reduces to quarter-power scaling under restrictive assumptions but would generally yield a scaling exponent somewhat less than three-quarters. For the whole tree, the model predicts that scaling between woody mass and foliage depends on crown ratio. Measurements on three boreal tree species are consistent with the model predictions.},
	language = {en},
	number = {12},
	urldate = {2024-08-22},
	journal = {Ecology},
	author = {Mäkelä, Annikki and Valentine, Harry T.},
	year = {2006},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1890/0012-9658\%282006\%2987\%5B2967\%3ACRIASI\%5D2.0.CO\%3B2},
	keywords = {allometry, biomass, branching network, fractal dimension, pipe model, quarter-power scaling},
	pages = {2967--2972},
	file = {Snapshot:/Users/tpc/Zotero/storage/SRM99JDK/0012-9658(2006)87[2967CRIASI]2.0.html:text/html},
}


@article{Bohlman2006,
	title = {Allometry, adult stature and regeneration requirement of 65 tree species on {Barro} {Colorado} {Island}, {Panama}},
	volume = {22},
	issn = {1469-7831, 0266-4674},
	url = {https://www.cambridge.org/core/journals/journal-of-tropical-ecology/article/abs/allometry-adult-stature-and-regeneration-requirement-of-65-tree-species-on-barro-colorado-island-panama/BD26715FBBCFEF537551DFE3F44B6407},
	doi = {10.1017/S0266467405003019},
	abstract = {This study provides a community-level analysis of how regeneration requirement and adult stature are related to tree allometry (diameter, height and crown size) throughout post-seedling ontogeny on Barro Colorado Island, Panama. Comparing 65 species, gap species are taller, have higher diameter growth rates and occupy more low-canopy sites (≤10 m canopy height) than shade species at small diameters (≤10 cm dbh). For trees {\textgreater}10 cm dbh, diameter-height relationships and growth rates no longer differ between gap and shade species, but shade species have larger, particularly deeper, crowns than gap species. Species with tall adult stature have more slender stems with larger crowns compared with treelet and mid-canopy species starting at 5 cm dbh. From 10 to 40 cm dbh, diameter growth rate is also significantly greater for tall species. The consistent allometric differences between functional groups on a community level will, in part, determine vertical and horizontal stand structure.},
	language = {en},
	number = {2},
	urldate = {2024-08-22},
	journal = {Journal of Tropical Ecology},
	author = {Bohlman, Stephanie and O'Brien, Sean},
	month = mar,
	year = {2006},
	keywords = {allometry, forest structure, functional groups, Panama, tree architecture},
	pages = {123--136},
}


