	keywords = {Climate, ectomycorrhizal associations, large tree density, pan-tropical analysis, soils, species traits, tree size, tropical forest biomass, wind dispersal, wood density},
	pages = {1261--1271},
	file = {Full Text PDF:/Users/tpc/Zotero/storage/KH5AM4FF/Slik et al. - 2013 - Large trees drive forest aboveground biomass varia.pdf:application/pdf;Snapshot:/Users/tpc/Zotero/storage/J4Y5663Z/geb.html:text/html},
